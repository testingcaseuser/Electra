  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-infinite-scroller": "^1.2.5",
    "@types/three": "^0.164.1",
    "eslint": "^8",
