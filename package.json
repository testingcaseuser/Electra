    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^20.19.10",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-infinite-scroller": "^1.2.5",
    "@types/three": "^0.164.1",
    "eslint": "^8.57.1",
